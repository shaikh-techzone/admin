<div class="main-panel">
    <div class="content-wrapper">
      <div class="row">
        <div class="col-sm-12">
          <div class="home-tab">
            <div class="d-sm-flex align-items-center justify-content-between border-bottom">
              <ul class="nav nav-tabs" role="tablist">
                <li class="nav-item">
                  <a class="nav-link active ps-0" id="home-tab" data-bs-toggle="tab" href="#overview"
                    role="tab" aria-controls="overview" aria-selected="true">Overview</a>
                </li>
               
              </ul>
              <div>
                <div class="btn-wrapper">
                  <a href="mailto:someone@example.com" class="btn btn-otline-dark align-items-center"><i class="icon-share"></i>
                    Share</a>
                  <button href="#" class="btn btn-otline-dark" onclick="p_page()"><i class="icon-printer"></i> Print</button>
                  <a href="#" class="btn btn-primary text-white me-0"><i class="icon-download"></i> Export</a>
                </div>
              </div>
            </div>
            <div class="tab-content tab-content-basic">
              <div class="tab-pane fade show active" id="overview" role="tabpanel" aria-labelledby="overview">
                <div class="row">
                  <div class="col-sm-12">
                    <div class="statistics-details d-flex align-items-center justify-content-between">
                      <div>
                        <p class="statistics-title">Total no. of Jobs</p>
                        <h3 class="rate-percentage"><%= noOfJobs%>.00</h3>
                        <!-- <p class="text-danger d-flex"><i class="mdi mdi-menu-down"></i><span>-0.5%</span></p> -->
                      </div>
                      <div>
                        <p class="statistics-title">Total no. of Internships</p>
                        <h3 class="rate-percentage"><%= noOfInternships%>.00</h3>
                        <!-- <p class="text-success d-flex"><i class="mdi mdi-menu-up"></i><span>+0.1%</span></p> -->
                      </div>
                      <div>
                        <p class="statistics-title">Total no. of Universities </p>
                        <h3 class="rate-percentage"><%= noOfUniversities%>.00</h3>
                        <!-- <p class="text-danger d-flex"><i class="mdi mdi-menu-down"></i><span>68.8</span></p> -->
                      </div>
                      <div class="d-none d-md-block">
                        <p class="statistics-title">Total no. of Colleges</p>
                        <h3 class="rate-percentage"><%= noOfColleges%>.00</h3>
                        <!-- <p class="text-success d-flex"><i class="mdi mdi-menu-down"></i><span>+0.8%</span></p> -->
                      </div>
                      <div class="d-none d-md-block">
                        <p class="statistics-title">Total no. of Scholarships</p>
                        <h3 class="rate-percentage">68.8</h3>
                        <!-- <p class="text-danger d-flex"><i class="mdi mdi-menu-down"></i><span>68.8</span></p> -->
                      </div>
                      
                    </div>
                  </div>
                </div>
                
                <div class="row">
                  <div class="col-lg-4 d-flex flex-column">
                      <div class="row flex-grow">
